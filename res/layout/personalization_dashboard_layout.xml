<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <!-- wallpaper, themes, toolbox layouts -->
    <LinearLayout
        android:id="@+id/quick_access_widgets"
        android:layout_width="match_parent"
        android:layout_height="320dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="12dp"
        android:layout_weight="1">

        <!-- Wallpapaper and Themes -->
        <com.google.android.material.card.MaterialCardView
            android:id="@+id/themes_card"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:layout_marginStart="24dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="@*android:color/transparent"
            android:clickable="true"
            android:clipToOutline="true"
            app:cardCornerRadius="33dp"
            app:cardElevation="0dp">

            <com.android.settings.utils.WallpaperView
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:contentDescription="@null"
                android:foreground="@drawable/wallpaper_scrim"
                android:background="@drawable/wallpaper_scrim"
                android:elevation="0dp" />

            <RelativeLayout
                android:id="@+id/themes_clock_frame"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:id="@+id/lockscreen_title"
                    android:textSize="18sp"
                    android:layout_above="@+id/lockscreen_clock_preview"
                    android:textColor="#FFFFFFFF"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="24dp"
                    android:text="@string/lockscreen_title"
                    android:layout_centerHorizontal="true" 
                    android:fontFamily="googlesans-flex"
                    android:textFontWeight="300"
                    android:ellipsize="end"
                    android:maxLines="1"
                    style="@style/op_control_text_style_h6" />

                <TextClock
                    android:id="@+id/lockscreen_clock_preview"
                    android:textSize="60sp"
                    android:textColor="@*android:color/system_accent1_100"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:format12Hour="hh\nmm"
                    android:format24Hour="hh\nmm"
                    android:fontFamily="@*android:string/config_clockFontFamily"
                    android:textFontWeight="300"
                    android:textAlignment="center"
                    android:layout_centerHorizontal="true" 
                    style="@style/op_control_text_style_h6" />

                <TextClock
                    android:id="@+id/lockscreen_clock_small_preview"
                    android:textSize="8sp"
                    android:textColor="@*android:color/system_accent1_100"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:format12Hour="EEE, d MMM"
                    android:format24Hour="EEE, d MMM"
                    android:fontFamily="@*android:string/config_clockFontFamily"
                    android:layout_below="@+id/lockscreen_clock_preview"
                    android:textFontWeight="300"
                    android:layout_marginTop="10dp"
                    android:layout_centerHorizontal="true" 
                    style="@style/op_control_text_style_h6" />

                <ImageView
                    android:id="@+id/lock_icon"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_marginTop="20dp"
                    android:src="@drawable/ic_lock"
                    android:scaleType="fitCenter"
                    android:adjustViewBounds="true"
                    android:tint="#FFFFFFFF"
                    android:layout_below="@+id/lockscreen_clock_small_preview"
                    android:layout_centerHorizontal="true" 
                    android:layout_marginStart="24dp" />

            </RelativeLayout>

        </com.google.android.material.card.MaterialCardView>
        <!-- System Themes -->
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/system_themes"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="24dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/custom_preference_background_oos"
                android:clickable="true"
                android:clipChildren="true"
                android:clipToPadding="true"
                android:clipToOutline="true">   
            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/theme_colors_animation"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:scaleType="centerCrop"
                android:alpha="0.5"
                app:lottie_autoPlay="true"
                app:lottie_loop="true"
                app:lottie_rawRes="@raw/rattan"/>
                 <View
                    android:id="@+id/system_themes_divider"
                    android:layout_width="2dp"
                    android:layout_height="2dp"
                    android:layout_centerInParent="true" />
                <TextView
                    android:textSize="18sp"
                    android:textColor="@color/top_level_preference_text_color_primary"
                    android:id="@+id/system_themes_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/themes_title"
                    android:layout_below="@+id/system_themes_divider"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="6dp"
                    style="@style/op_control_text_style_h6" />
                <TextView
                    android:textSize="11.5sp"
                    android:textColor="@color/top_level_preference_text_color_secondary"
                    android:id="@+id/system_themes_summary"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/themes_summary"
                    android:layout_below="@+id/system_themes_title"
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="20dp"
                    style="@style/op_control_text_style_h6" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/toolbox"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="24dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/custom_preference_background_oos"
                android:clickable="true"
                android:clipChildren="true"
                android:clipToPadding="true"
                android:clipToOutline="true">
            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/toolbox_colors_animation"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:scaleType="centerCrop"
                android:alpha="0.5"
                app:lottie_autoPlay="true"
                app:lottie_loop="true"
                app:lottie_rawRes="@raw/amber"/>
                 <View
                    android:id="@+id/toolbox_divider"
                    android:layout_width="2dp"
                    android:layout_height="2dp"
                    android:layout_centerInParent="true" />
                <TextView
                    android:textSize="18sp"
                    android:textColor="@color/top_level_preference_text_color_primary"
                    android:id="@+id/toolbox_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/toolbox_title"
                    android:layout_below="@+id/toolbox_divider"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="6dp"
                    style="@style/op_control_text_style_h6" />
                <TextView
                    android:textSize="11.5sp"
                    android:textColor="@color/top_level_preference_text_color_secondary"
                    android:id="@+id/toolbox_summary"
                    android:layout_marginTop="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/toolbox_summary"
                    android:layout_below="@+id/toolbox_title"
                    android:layout_marginStart="20dp"
                    style="@style/op_control_text_style_h6" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/colors_font"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        <RelativeLayout
            android:id="@+id/colors"
            android:layout_width="0dp"
            android:layout_height="140dp"
            android:layout_weight="1"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/custom_preference_background_oos"
            android:clickable="true"
            android:clipChildren="true"
            android:clipToPadding="true"
            android:clipToOutline="true">
            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/colors_animation"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:scaleType="centerCrop"
                android:alpha="0.5"
                app:lottie_autoPlay="true"
                app:lottie_loop="true"
                app:lottie_rawRes="@raw/peach"/>
                 <View
                    android:id="@+id/colors_divider"
                    android:layout_width="2dp"
                    android:layout_height="2dp"
                    android:layout_centerInParent="true" />
                <TextView
                    android:textSize="18sp"
                    android:textColor="@color/top_level_preference_text_color_primary"
                    android:id="@+id/colors_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/colors_title"
                    android:layout_below="@+id/colors_divider"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="2dp"
                    style="@style/op_control_text_style_h6" />
                <TextView
                    android:textSize="11.5sp"
                    android:textColor="@color/top_level_preference_text_color_secondary"
                    android:id="@+id/colors_summary"
                    android:layout_marginTop="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/colors_summary"
                    android:layout_below="@+id/colors_title"
                    android:layout_marginStart="20dp"
                    style="@style/op_control_text_style_h6" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/fonts"
                android:layout_width="0dp"
                android:layout_height="140dp"
                android:layout_weight="1"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/custom_preference_background_oos"
                android:clickable="true"
                android:clipChildren="true"
                android:clipToPadding="true"
                android:clipToOutline="true">
                <TextView
                    android:textSize="18sp"
                    android:textColor="@color/top_level_preference_text_color_primary"
                    android:id="@+id/fonts_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/fonts_title"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="14dp"
                    style="@style/op_control_text_style_h6" />
                <TextView
                    android:textSize="18sp"
                    android:textColor="@color/top_level_preference_text_color_primary"
                    android:id="@+id/fonts_summary"
                    android:layout_marginTop="8dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/fonts_summary"
                    android:layout_below="@+id/fonts_title"
                    android:layout_marginStart="20dp"
                    style="@style/op_control_text_style_h6" />
            </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/clock_face_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <RelativeLayout
            android:id="@+id/clock_face"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:background="@drawable/custom_preference_background_oos"
            android:layout_marginBottom="20dp"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            android:clickable="true"
            android:clipChildren="true"
            android:clipToPadding="true"
            android:clipToOutline="true">
            <TextView
                android:id="@+id/clock_face_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="28dp"
                android:text="@string/theme_customization_lock_clock_title"
                android:textSize="18sp"
                android:textColor="@color/top_level_preference_text_color_primary"
                style="@style/op_control_text_style_h6" />
            <TextView
                android:id="@+id/clock_face_summary"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/clock_face_title"
                android:layout_alignStart="@id/clock_face_title"
                android:layout_toStartOf="@id/clock_face_icon"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="4dp"
                android:text="@string/theme_customization_lock_clock_summary"
                android:textSize="13sp"
                android:textColor="@color/top_level_preference_text_color_secondary"
                style="@style/op_control_text_style_h6" />
                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/clock_face_icon"
                    android:layout_width="60dp"
                    android:layout_height="80dp"
                    android:background="@*android:color/transparent"
                    android:clickable="false"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="16dp"
                    android:clipToOutline="true"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="0dp">
                    <com.android.settings.utils.WallpaperView
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop"
                        android:contentDescription="@null"
                        android:foreground="@drawable/wallpaper_scrim"
                        android:background="@drawable/wallpaper_scrim"
                        android:elevation="0dp" />
                    <RelativeLayout
                        android:id="@+id/clock_face_clock_frame"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center">
                        <TextClock
                            android:id="@+id/clock_face_clock_preview"
                            android:textSize="28sp"
                            android:textColor="@*android:color/system_accent1_100"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:format12Hour="hh\nmm"
                            android:format24Hour="hh\nmm"
                            android:fontFamily="@*android:string/config_clockFontFamily"
                            android:textFontWeight="300"
                            android:textAlignment="center"
                            android:layout_centerHorizontal="true" 
                            style="@style/op_control_text_style_h6" />
                    </RelativeLayout>
                </com.google.android.material.card.MaterialCardView>
        </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/whats_new_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <RelativeLayout
            android:id="@+id/whats_new"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:background="@drawable/custom_preference_background_oos"
            android:layout_marginBottom="20dp"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            android:clickable="true"
            android:clipChildren="true"
            android:clipToPadding="true"
            android:clipToOutline="true">
            <TextView
                android:id="@+id/whats_new_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:text="@string/changelog_rising_title"
                android:textSize="18sp"
                android:textColor="@color/top_level_preference_text_color_primary"
                style="@style/op_control_text_style_h6" />
            <TextView
                android:id="@+id/whats_new_summary"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/whats_new_title"
                android:layout_alignStart="@id/whats_new_title"
                android:layout_toStartOf="@id/whats_new_icon"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="4dp"
                android:text="@string/changelog_rising_summary"
                android:textSize="13sp"
                android:textColor="@color/top_level_preference_text_color_secondary"
                style="@style/op_control_text_style_h6" />
            <com.android.settings.utils.DynamicIconPreferenceView
                android:id="@+id/whats_new_icon"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="16dp"
                android:src="@drawable/ic_changelog" />
        </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/misc_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:paddingStart="20dp"
        android:paddingEnd="20dp"
        android:paddingBottom="20dp"
        android:weightSum="3">
        <RelativeLayout
            android:id="@+id/quicksettings"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:layout_weight="1"
            android:background="@drawable/custom_preference_background_oos"
            android:clickable="true"
            android:clipChildren="true"
            android:clipToPadding="true"
            android:clipToOutline="true">
            <com.android.settings.utils.DynamicIconPreferenceView
                android:id="@+id/quicksettings_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:src="@drawable/ic_settings_quicksettings" />
            <TextView
                android:id="@+id/quicksettings_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/quicksettings_icon"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="6dp"
                android:text="@string/quicksettings_title"
                android:textSize="13sp"
                android:ellipsize="end"
                android:maxLines="1"
                android:textColor="@color/top_level_preference_text_color_primary"
                style="@style/op_control_text_style_h6" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/status_bar"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:layout_weight="1"
            android:background="@drawable/custom_preference_background_oos"
            android:clickable="true"
            android:clipChildren="true"
            android:clipToPadding="true"
            android:clipToOutline="true"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">
            <com.android.settings.utils.DynamicIconPreferenceView
                android:id="@+id/status_bar_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:ellipsize="end"
                android:maxLines="1"
                android:src="@drawable/ic_settings_statusbar" />
            <TextView
                android:id="@+id/status_bar_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/status_bar_icon"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="6dp"
                android:text="@string/sb_settings_title"
                android:textSize="13sp"
                android:ellipsize="end"
                android:maxLines="1"
                android:textColor="@color/top_level_preference_text_color_primary"
                style="@style/op_control_text_style_h6" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/gestures"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:layout_weight="1"
            android:background="@drawable/custom_preference_background_oos"
            android:clickable="true"
            android:clipChildren="true"
            android:clipToPadding="true"
            android:clipToOutline="true">
            <com.android.settings.utils.DynamicIconPreferenceView
                android:id="@+id/gestures_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:src="@drawable/ic_settings_gestures" />
            <TextView
                android:id="@+id/gestures_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/gestures_icon"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="6dp"
                android:text="@string/gestures_title"
                android:ellipsize="end"
                android:maxLines="1"
                android:textSize="13sp"
                android:textColor="@color/top_level_preference_text_color_primary"
                style="@style/op_control_text_style_h6" />
        </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/misc_container2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:paddingStart="20dp"
        android:paddingEnd="20dp"
        android:paddingBottom="20dp"
        android:weightSum="3">
        <RelativeLayout
            android:id="@+id/icon_shapes"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:layout_weight="1"
            android:background="@drawable/custom_preference_background_oos"
            android:clickable="true"
            android:clipChildren="true"
            android:clipToPadding="true"
            android:clipToOutline="true">
            <com.android.settings.utils.DynamicIconPreferenceView
                android:id="@+id/icons_shape_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:src="@drawable/ic_shapes" />
            <TextView
                android:id="@+id/theme_customization_icon_shape_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/icons_shape_icon"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="6dp"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="@string/theme_customization_icon_shape_title"
                android:textSize="13sp"
                android:textColor="@color/top_level_preference_text_color_primary"
                style="@style/op_control_text_style_h6" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/notifications"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:layout_weight="1"
            android:background="@drawable/custom_preference_background_oos"
            android:clickable="true"
            android:clipChildren="true"
            android:clipToPadding="true"
            android:clipToOutline="true"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">
            <com.android.settings.utils.DynamicIconPreferenceView
                android:id="@+id/notification_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:ellipsize="end"
                android:maxLines="1"
                android:src="@drawable/ic_custom_notifications" />
            <TextView
                android:id="@+id/notifications_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/notification_icon"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="6dp"
                android:text="@string/notifications_title"
                android:textSize="13sp"
                android:ellipsize="end"
                android:maxLines="1"
                android:textColor="@color/top_level_preference_text_color_primary"
                style="@style/op_control_text_style_h6" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/sounds"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:layout_weight="1"
            android:background="@drawable/custom_preference_background_oos"
            android:clickable="true"
            android:clipChildren="true"
            android:clipToPadding="true"
            android:clipToOutline="true">
            <com.android.settings.utils.DynamicIconPreferenceView
                android:id="@+id/sounds_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:src="@drawable/ic_custom_sound" />
            <TextView
                android:id="@+id/sound_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/sounds_icon"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="6dp"
                android:text="@string/sound_title"
                android:ellipsize="end"
                android:maxLines="1"
                android:textSize="13sp"
                android:textColor="@color/top_level_preference_text_color_primary"
                style="@style/op_control_text_style_h6" />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
